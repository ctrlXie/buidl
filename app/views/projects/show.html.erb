<div class="wrapper">
  <div class="profile_header">
    <%= image_tag 'stock_cover_photo2.jpg', class: "cover_photo" %>
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-3">
          <div class="profile-img profile-photo" style="background-image: url('<%= image_path("logo.png") %>');">
          </div>
        </div>
        <div class="col-xs-12 col-sm-6">
          <div class="information">
            <h1 class="name"><%= @project_details["name"] %></h1>
            <p class="bio"><%= @project_details["description"] %></p>
          </div>
        </div>
      </div>








      <div class="text-center">
        <p> <%= link_to "Home", root_path %></p>
      </div>
    </div>
  </div>
</div>
